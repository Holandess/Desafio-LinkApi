{
  "name": "desafio-linkapi",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": "Filipe Holanda <felipe.holandes17@gmail.com>",
  "license": "MIT",
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "runtime": "firebase functions:config:get > .runtimeconfig.json",
    "serve": "yarn runtime && yarn build && firebase serve --only functions",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly --ignore-watch node_modules --no-notify src/server.ts",
    "test": "jest"
  },
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@shelf/jest-mongodb": "^1.2.1",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.0",
    "@types/mongoose": "^5.7.36",
    "@types/urlencode": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "firebase-functions-test": "^0.2.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^9.0.0",
    "firebase-functions": "^3.9.0",
    "mongoose": "^5.9.28",
    "urlencode": "^1.1.0"
  }
}